import org.newdawn.slick.Image;
import org.newdawn.slick.SlickException;
import org.newdawn.slick.Sound;


public class Elixir extends Item{
	
	public static final String IMAGE = "assets/items/elixir.png";
	public static final String NAME = "Elixir of Life";
	public static final String PICKUP = "assets/m_health.wav";
	
	public Elixir(double start_x, double start_y) throws SlickException
	{
		this.img = new Image(IMAGE);
		this.name = NAME;
		this.x = start_x;
		this.y = start_y;
		this.pickup_noise = new Sound(PICKUP);
	}
}
